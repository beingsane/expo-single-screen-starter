{
  "expo": {
    "name": "expo-single-screen-starter",
    "description": "simple react native single screen application starter with expo",
    "slug": "expo-single-screen-starter",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.2",
    "orientation": "portrait",
    "primaryColor": "#1d2d3a",
    "icon": "src/assets/icon.png",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1d2d3a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["src/assets/**/*"],
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "backgroundColor": "#1d2d3a"
    }
  }
}
